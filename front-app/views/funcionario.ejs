<h1><%= title%></h1>

<table>
    <thead>
        <tr>
            <%for(let col of cols){%>
            <th><%=col%></th>
            <%}%>
        </tr>
    </thead>
    <tbody>
        <%for(let funcionario of funcionarios){%>
            <tr>
                <%for(let prop in funcionario){%>
                    <td><%=funcionario[prop]%></td>
                <%}%>
            </tr>
        <%}%>
    </tbody>


</table>